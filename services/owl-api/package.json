{
  "name": "@thr1ve/owl-api",
  "private": true,
  "version": "0.0.0",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -d",
    "start": "ts-node src/index.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@thr1ve/owl-api-binding": "^0.0.0",
    "apollo-link": "1.2.1",
    "apollo-link-http": "1.3.3",
    "cross-fetch": "1.1.1",
    "dataloader": "^1.4.0",
    "graphql": "0.13.1",
    "graphql-binding": "1.2.3",
    "graphql-tools": "2.21.0",
    "graphql-yoga": "^1.3.6",
    "subscriptions-transport-ws": "0.9.6",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/graphql": "0.12.4",
    "@types/jest": "^22.1.3",
    "@types/node": "9.4.6",
    "axios": "^0.18.0",
    "dotenv": "^5.0.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "ts-jest": "^22.4.0",
    "ts-node": "^5.0.0",
    "typescript": "2.7.2"
  }
}
