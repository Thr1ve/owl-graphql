{
  "extends": "plugin:react-app/recommended",
  "plugins": ["jest", "prettier"],
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "curly": "warn",
    "prettier/prettier": ["error", { "singleQuote": true }]
  }
}
